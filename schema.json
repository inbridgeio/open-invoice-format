    "IBAN": {
      "type": "string",
      "description": "The International Bank Account Number (IBAN) is an international standard for identifying bank accounts.",
      "pattern": "^[A-Z]{2}[0-9]{2}[A-Z0-9]{1,30}$"
    },
    "BIC": {
      "type": "string",
      "description": "The Business Identifier Code (BIC) is an international standard for identifying business entities.",
      "pattern": "^[A-Z]{6}[A-Z0-9]{2}([A-Z0-9]{3})?$"
    },
    "DUNS": {
      "type": "string",
      "description": "The D-U-N-S Number is a unique nine-digit identifier for businesses.",
      "pattern": "^[0-9]{9}$"
    },
    "InvoiceId": {
